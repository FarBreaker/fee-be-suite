{
	"prefix": "fee-suite",
	"env": {
		"region": "eu-central-1",
		"stage": "dev"
	},
	"compute": {
		"lambda": {
			"profile": "performance",
			"timeout": 30,
			"memory": 1024
		}
	},
	"persistance": {
		"table": {
			"tableName": "fee-suite-table-dev",
			"removalPolicy": "destroy"
		},
		"bucket": {
			"bucketName": "fee-suite-bucket-dev",
			"removalPolicy": "destroy",
			"autoDeleteObjects": true
		}
	},
	"network": {
		"apigw": {
			"corsPreflight": {
				"allowMethods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
				"allowOrigins": ["*"],
				"allowHeaders": ["*"]
			}
		}
	}
}
