{
	"name": "symphony",
	"version": "0.1.3",
	"bin": {
		"symphony": "bin/symphony.js"
	},
	"type": "module",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"cdk": "cdk",
		"docs": "npx typedoc",
		"deploy:dev": "cdk deploy --require-approval never --all --outputs-file out.json --context env=dev",
		"deploy:prod": "cdk deploy --require-approval never --all --outputs-file out.json --context env=prod",
		"test:e2e": "jest --config ./jest.config.e2e.js --detectOpenHandles --runInBand",
		"test:e2e:watch": "jest --config ./jest.config.e2e.js --watch --detectOpenHandles --runInBand"
	},
	"devDependencies": {
		"@aws-sdk/client-dynamodb": "^3.830.0",
		"@aws-sdk/lib-dynamodb": "^3.830.0",
		"@aws-sdk/lib-storage": "^3.832.0",
		"@types/aws-lambda": "^8.10.150",
		"@types/busboy": "^1.5.4",
		"@types/jest": "^30.0.0",
		"@types/node": "24.0.3",
		"aws-cdk": "^2.1019.1",
		"aws-lambda": "^1.0.7",
		"aws-sdk": "^2.1692.0",
		"busboy": "^1.6.0",
		"cdk-nag": "^2.36.23",
		"esbuild": "^0.25.5",
		"jest": "^30.0.2",
		"ts-jest": "^29.4.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.6",
		"typedoc": "^0.28.5",
		"typescript": "~5.8.3"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.832.0",
		"aws-cdk-lib": "2.202.0",
		"constructs": "^10.4.2",
		"file-type": "^21.0.0",
		"source-map-support": "^0.5.21",
		"uuid": "^11.1.0"
	},
	"volta": {
		"node": "18.15.0"
	}
}
