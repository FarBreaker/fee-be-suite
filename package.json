{
	"name": "symphony",
	"version": "0.1.3",
	"bin": {
		"symphony": "bin/symphony.js"
	},
	"type": "module",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"cdk": "cdk",
		"docs": "npx typedoc",
		"deploy:dev": "cdk deploy --require-approval never --all --outputs-file out.json --context env=dev",
		"deploy:prod": "cdk deploy --require-approval never --all --outputs-file out.json --context env=prod",
		"test:e2e": "jest --config ./jest.config.e2e.js --detectOpenHandles --runInBand",
		"test:e2e:watch": "jest --config ./jest.config.e2e.js --watch --detectOpenHandles --runInBand"
	},
	"devDependencies": {
		"@aws-sdk/client-dynamodb": "^3.788.0",
		"@aws-sdk/lib-dynamodb": "^3.789.0",
		"@types/aws-lambda": "^8.10.149",
		"@types/jest": "^29.5.14",
		"@types/node": "22.14.1",
		"aws-cdk": "^2.1010.0",
		"aws-lambda": "^1.0.7",
		"aws-sdk": "^2.1692.0",
		"cdk-nag": "^2.35.72",
		"esbuild": "^0.25.2",
		"jest": "^29.7.0",
		"ts-jest": "^29.3.2",
		"ts-node": "^10.9.2",
		"typedoc": "^0.28.2",
		"typescript": "~5.8.3"
	},
	"dependencies": {
		"aws-cdk-lib": "2.190.0",
		"constructs": "^10.4.2",
		"source-map-support": "^0.5.21"
	},
	"volta": {
		"node": "18.15.0"
	}
}
